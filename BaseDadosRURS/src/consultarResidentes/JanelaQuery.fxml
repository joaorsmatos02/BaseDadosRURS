<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.Button?>

<StackPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="consultarResidentes.JanelaQueryController">
	<!-- Caixa de texto -->
	<TextArea fx:id="queryArea" promptText="Insira a sua query aqui"/>
	
	<!-- Botao ok -->
	<Button fx:id="okQueryButton" alignment="bottom_right"
		    text="OK" onAction="#okQueryButtonHandler"/>
</StackPane>

